{
    "name": "cranelift",
    "displayName": "cranelift",
    "description": "Language support for Cranelift IR.",
    "version": "0.0.1",
    "publisher": "gregoire",
    "readme": "README.md",
    "author": {
        "name": "Gr√©goire Geis",
        "email": "git@gregoirege.is"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/6A/cranelift-vscode"
    },
    "bugs": {
        "url": "https://github.com/6A/cranelift-vscode/issues"
    },
    "homepage": "https://github.com/6A/cranelift-vscode",
    "license": "MIT",
    "main": "out/extension.js",
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "dependencies": {
        "vscode": "^1.1.4"
    },
    "devDependencies": {
        "typescript": "^2.1.5"
    },
    "engines": {
        "vscode": "^1.21.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:cranelift"
    ],
    "contributes": {
        "languages": [
            {
                "id": "cranelift",
                "aliases": [
                    "Cranelift"
                ],
                "extensions": [
                    ".clif",
                    ".clift",
                    ".cranelift"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "cranelift",
                "scopeName": "source.cranelift",
                "path": "./syntaxes/cranelift.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "cranelift",
                "path": "./snippets/cranelift.json"
            }
        ]
    }
}
